<p id="notice"><%= notice %></p>

<h1>Api Tarifas</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_tarifas.each do |api_tarifa| %>
      <tr>
        <td><%= link_to 'Show', api_tarifa %></td>
        <td><%= link_to 'Edit', edit_api_tarifa_path(api_tarifa) %></td>
        <td><%= link_to 'Destroy', api_tarifa, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

