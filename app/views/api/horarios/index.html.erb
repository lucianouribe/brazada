<p id="notice"><%= notice %></p>

<h1>Api Horarios</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @api_horarios.each do |api_horario| %>
      <tr>
        <td><%= link_to 'Show', api_horario %></td>
        <td><%= link_to 'Edit', edit_api_horario_path(api_horario) %></td>
        <td><%= link_to 'Destroy', api_horario, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Api Horario', new_api_horario_path %>
